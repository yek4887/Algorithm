#데이터 삽입(insert_data)

kakaotalk = ["가을","레이","원영","리즈","이서"]

def insert_data(position,friend):#매개변수로 삽입할 위치, 데이터 받음
  
  if position < 0 or position >len(kakaotalk):
    print("데이터를 삽입할 범위를 벗어났습니다.")
    return
  
  kakaotalk.append(None) #빈칸 추가 >> [...."이서",None]
  k_len = len(kakaotalk) #배열의 현재 크기 계산
  print(k_len)
  for i in range(k_len-1, position, -1): 
    """k_len-1 : 마지막 위치, position : 삽입할 위치"""
    kakaotalk[i] = kakaotalk[i-1] #i-1 위치의 인덱스가 i 위치로 이동
    kakaotalk[i-1] = None
  
  kakaotalk[position] = friend
    
insert_data(3, '유진')
print(kakaotalk)
